{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cod",
  "version": "0.1.0",
  "identifier": "com.cod.app",
  "build": {
    "beforeDevCommand": "npm run dev",
      "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "visible": true,
        "title": "cod",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "titleBarStyle": "Visible",
        "url": "/",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "copyright": "Â© 2025 cod",
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
